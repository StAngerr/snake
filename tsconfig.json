{
  "compilerOptions": {
    "target": "es2015",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "lib": ["es6", "dom"],
    "module": "es2015"
  },
  "exclude": ["node_modules"]
}
